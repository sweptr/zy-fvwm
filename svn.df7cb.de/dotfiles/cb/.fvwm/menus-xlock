+	"helix:	Shows string art." Exec xlock -nolock -mode helix
+	"hop:	Shows real plane iterated fractals." Exec xlock -nolock -mode hop
+	"hyper:	Shows a spinning tesseract in 4D space." Exec xlock -nolock -mode hyper
+	"ico:	Shows a bouncing polyhedra." Exec xlock -nolock -mode ico
+	"ifs:	Shows a modified iterated function system." Exec xlock -nolock -mode ifs
+	"image:	Shows randomly appearing logos." Exec xlock -nolock -mode image
+	"julia:	Shows the Julia set." Exec xlock -nolock -mode julia
+	"kaleid:	Shows a kaleidoscope." Exec xlock -nolock -mode kaleid
+	"laser:	Shows spinning lasers." Exec xlock -nolock -mode laser
+	"life:	Shows Conway's game of life." Exec xlock -nolock -mode life
+	"life1d:	Shows Wolfram's game of 1D life." Exec xlock -nolock -mode life1d
+	"life3d:	Shows Bays' game of 3D life." Exec xlock -nolock -mode life3d
+	"lightning:	Shows Keith's fractal lightning bolts." Exec xlock -nolock -mode lightning
+	"lisa:	Shows animated lissajous loops." Exec xlock -nolock -mode lisa
+	"lissie:	Shows lissajous worms." Exec xlock -nolock -mode lissie
+	"loop:	Shows Langton's self-producing loops." Exec xlock -nolock -mode loop
+	"" Nop
+	"(1)	Not compiled in by default." Nop
+	"(2)	May not be available depending on how it was configured." Nop
+	"(3)	Does not show up in random mode." Nop

