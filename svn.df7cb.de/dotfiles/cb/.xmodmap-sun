keycode 0x08 =	Caps_Lock
keycode 0x43 =	U	NoSymbol	udiaeresis
keycode 0x45 =	O	NoSymbol	odiaeresis
keycode 0x4C =	Up	KP_8	F28
keycode 0x4D =	KP_Prior	KP_9	F29	Prior
keycode 0x54 =	A	NoSymbol	adiaeresis
keycode 0x55 =	S	NoSymbol	ssharp
keycode 0x62 =	Left	KP_4	F30
keycode 0x63 =	KP_5	KP_5	F31
keycode 0x64 =	Right	KP_6	F32
keycode 0x78 =	Down	KP_2	F34
keycode 0x79 =	KP_Next	KP_3	F35	Next
keycode 0x7E =	Mode_switch

clear Lock
clear Control
clear Mod1
clear Mod2
clear Mod3
clear Mod4
clear Mod5

add	Control	= Control_L Control_R
add	Mod1	= Alt_L
add	Mod2	= Num_Lock
add	Mod3	= Mode_switch
add	Mod4	= Meta_R Meta_L
