remove Lock    = Caps_Lock
remove Mod1    = Alt_R

keycode 0x3C =	U	NoSymbol	Udiaeresis
keycode 0x44 =	O	NoSymbol	Odiaeresis
keycode 0x1C =	A	NoSymbol	Adiaeresis
keycode 0x1B =	S	NoSymbol	ssharp
keycode 0x14 =	Control_L
keycode 0x39 =	Mode_switch

add    Control = Control_L
add    Mod2    = Mode_switch
